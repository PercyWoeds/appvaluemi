<p id="notice"><%= notice %></p>

<h1>Listing Ocupacions</h1>

<table>
  <thead>
    <tr>
     
      <th>Code</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ocupacions.each do |ocupacion| %>
      <tr>
     
        <td><%= ocupacion.code %></td>
        <td><%= ocupacion.name %></td>
        <td><%= link_to 'Show', ocupacion %></td>
        <td><%= link_to 'Edit', edit_ocupacion_path(ocupacion) %></td>
        <td><%= link_to 'Destroy', ocupacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ocupacion', new_ocupacion_path %>
