<h3 class="ib">
  Operaciones inventario
  <%= income.inventory_tag %>
</h3>
<%= income.enable_disable_inventory_button %>

<p class="center inventories-buttons">
  <%= render 'incomes/inventory_buttons', income: income %>
</p>

<ul class="listing unstyled inventories">
  <%= render partial: 'inventories/movement', collection: @income.inventories.includes(:store, :creator).order('date desc, id desc'), as: :inventory %>
</ul>


