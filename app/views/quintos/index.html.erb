<p id="notice"><%= notice %></p>

<h1>Quinta Categoria</h1>

<table>
  <thead>
    <tr>
      <th>Anio</th>
      <th>Employee</th>
      <th>Mes</th>
      <th>Renta impo ret</th>
      <th>Mes pendiente</th>
      <th>Retencion mensual</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quintos.each do |quinto| %>
      <tr>
        <td><%= quinto.anio %></td>
        <td><%= quinto.employee.full_name %></td>
        <td><%= quinto.mes %></td>
        <td><%= money(quinto.renta_impo_ret) %></td>
        <td><%= quinto.mes_pendiente %></td>
        <td><%= money(quinto.retencion_mensual) %></td>
        <td><%= link_to 'Ver', quinto %></td>
        <td><%= link_to 'Editar', edit_quinto_path(quinto) %></td>
        <td><%= link_to 'Eliminar', quinto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quinto', new_quinto_path %>
