<%= simple_form_for(@expense) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :code %>
    <%= f.input :gasto_id %>
    <%= f.input :documento %>
    <%= f.input :descrip %>
    <%= f.input :importe %>
    <%= f.input :fecha %>
    <%= f.input :document_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
